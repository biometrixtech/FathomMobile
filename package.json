{
  "name": "fathom",
  "version": "1.3.0",
  "private": true,
  "author": "Vir Desai",
  "scripts": {
    "fathom": "./fathom.sh",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone X'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pretest": "./node_modules/.bin/eslint -c .eslintrc --quiet --ext .js src",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "rnpm": {
    "assets": [
      "./src/constants/assets/fonts"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button|react-clone-referenced-element|react-navigation|redux-persist|react-native-vector-icons)"
    ],
    "testRegex": ".*test.js$",
    "collectCoverage": true,
    "coverageDirectory": "__coverage__",
    "verbose": true,
    "bail": true
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.5",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-android-location-services-dialog-box": "^2.5.0",
    "react-native-ble-manager": "^6.3.3",
    "react-native-code-push": "^5.3.5",
    "react-native-collapsible": "^0.12.0",
    "react-native-device-info": "^0.21.5",
    "react-native-easy-toast": "^1.1.0",
    "react-native-egg": "^1.0.1",
    "react-native-elements": "^0.19.1",
    "react-native-fabric": "^0.5.1",
    "react-native-modal-dropdown": "^0.6.2",
    "react-native-modalbox": "^1.5.0",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "tcomb-form-native": "^0.6.13",
    "uuid-by-string": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.2.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.3.1",
    "redux-logger": "^3.0.6"
  }
}
