    "react-native-svg": "^6.3.1",
    "react-native-swipeable": "^0.6.0",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tcomb-form-native": "^0.6.11"
