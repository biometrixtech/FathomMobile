{
  "name": "fathom",
  "version": "2.5.0-beta.7",
  "private": true,
  "author": "Mazen Chami",
  "scripts": {
    "android": "react-native run-android",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "fathom": "./fathom.sh",
    "ios": "react-native run-ios --simulator='iPhone X'",
    "pretest": "./node_modules/.bin/eslint -c .eslintrc --quiet --ext .js src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button|react-clone-referenced-element|react-navigation|redux-persist|react-native-vector-icons)"
    ],
    "testRegex": ".*test.js$",
    "collectCoverage": false,
    "coverageDirectory": "__coverage__",
    "verbose": false,
    "bail": true
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "lottie-react-native": "^2.5.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-native-action-button": "^2.8.5",
    "react-native-android-location-services-dialog-box": "2.6.0",
    "react-native-app-intro-slider": "^1.0.1",
    "react-native-ble-manager": "6.5.0",
    "react-native-calendar-strip": "^1.3.3",
    "react-native-collapsible": "^0.13.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "0.22.2",
    "react-native-dropdownalert": "^3.5.0",
    "react-native-easy-toast": "^1.1.0",
    "react-native-egg": "^1.0.1",
    "react-native-elements": "^0.19.1",
    "react-native-fabric": "0.5.1",
    "react-native-google-analytics-bridge": "^6.1.0",
    "react-native-keyboard-aware-scrollview": "^2.0.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-modal-dropdown": "^0.6.2",
    "react-native-modalbox": "^1.6.0",
    "react-native-progress": "^3.5.0",
    "react-native-push-notification": "3.0.2",
    "react-native-redux-connectivity": "^0.1.0",
    "react-native-remote-svg": "^1.2.0",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-sectioned-multi-select": "^0.5.2",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-splash-screen": "3.1.1",
    "react-native-vector-icons": "^6.1.0",
    "react-native-video": "3.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "rn-sliding-up-panel": "^1.3.0",
    "tcomb-form-native": "^0.6.15",
    "uuid-by-string": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "babel-bridge": "^1.12.11",
    "babel-eslint": "^8.2.6",
    "babel-preset-react-native": "^5.0.2",
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.4.1",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.4.1",
    "redux-logger": "^3.0.6"
  }
}
