'use strict';

var _reactNative = require('react-native');

var RNNetworkInfo = _reactNative.NativeModules.RNNetworkInfo;


var NetworkInfo = {
  getSSID: function getSSID(ssid) {
    RNNetworkInfo.getSSID(ssid);
  },
  getBSSID: function getBSSID(bssid) {
    RNNetworkInfo.getBSSID(bssid);
  },
  getIPAddress: function getIPAddress(ip) {
    RNNetworkInfo.getIPAddress(ip);
  },
  getIPV4Address: function getIPV4Address(ip) {
    RNNetworkInfo.getIPV4Address(ip);
  }
};

module.exports = { NetworkInfo: NetworkInfo };